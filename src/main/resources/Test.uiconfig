?PosX1(20%W)
?PosX2(80%W)
?PosY1(20%H)
?PosY2(80%H)
?Moving(false)
?Box(0)

color(255 255 200)
Background()

mode(.center)
*IF(.hold false $Moving(false))
*IF(.hold false $Box(0))

color(#FF0000)
weight(10)
/*
pos(@PosX2 @PosY2)
Line(@PosX1 @PosY1)
pos(@PosX1 @PosY2)
Line(@PosX2 @PosY1)
*/
pos(@PosX1 @PosY2)
Line(@PosX1 @PosY1)
pos(@PosX2 @PosY1)
Line(@PosX1 @PosY1)
pos(@PosX2 @PosY2)
Line(@PosX1 @PosY2)
pos(@PosX2 @PosY2)
Line(@PosX2 @PosY1)

pos(@PosX1 @PosY1)
Oval(50 50)
*IF(@Moving true *IF(@Box 1 $PosX1(.mouseX)))
*IF(@Moving true *IF(@Box 1 $PosY1(.mouseY)))
*IF(@Moving false *HOLD(50 50 $Box(1)))
*IF(@Moving false *HOLD(50 50 $Moving(true)))

pos(@PosX2 @PosY2)
Oval(50 50)
*IF(@Moving true *IF(@Box 2 $PosX2(.mouseX)))
*IF(@Moving true *IF(@Box 2 $PosY2(.mouseY)))
*IF(@Moving false *HOLD(50 50 $Box(2)))
*IF(@Moving false *HOLD(50 50 $Moving(true)))